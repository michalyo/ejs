<section class="courses">
    <h1>Take a Look at Our Courses</h1>
    <div class="grid">
        <% coursesArr.forEach(course =>{ %>
            <div class="grid-item">
                <h3><%= course.courseName %></h3>
                <p><%= course.title %></p>
                <a href="/purchased/drop/<%= course.id %> ">i dont want this course</a>
            </div>
        <% }) %>
    </div>
</section>